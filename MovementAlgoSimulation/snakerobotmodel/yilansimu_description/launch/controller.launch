<launch>

<rosparam file="$(find yilansimu_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="yilansimu" args="Revolute 30_position_controller Revolute 31_position_controller Revolute 32_position_controller Revolute 33_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/yilansimu/joint_states"/>
</node>

</launch>